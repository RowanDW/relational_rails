<%= form_with url: '/farms', method: :post, local: true do |form| %>
  <p>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </p>
  <p>
    <%= form.label :acres %>
    <%= form.number_field :acres %>
  </p>
  <p>
    <%= form.label :organic %>
    <%= form.check_box :organic %>
  </p>
  <p>
    <%= form.submit 'Create Farm' %>
  </p>
<% end %>
